{
  "clientId": "chain-effect-sim",
  "kafkaHost": "localhost:3501",
  "schemaRegistry": "http://localhost:3502",
  "wrapUnions": "auto",
  "heartbeatInterval": 10000,
  "fromOffset": true,
  "fetchAllSchemas": false,
  "fetchAllVersions": false,
  "autoRegisterSchemas": false,
  "schemaFolder": "./data/schemas",
  "consume": [{
    "topic": "chain_scenario",
    "offset": -1
  }, {
    "topic": "chain_flood",
    "offset": -1
  }, {
    "topic": "chain_power",
    "offset": -1
  }, {
    "topic": "chain_waterheight",
    "offset": -1
  }],
  "produce": ["chain_scenario", "chain_flood", "chain_power", "chain_waterheight"]
}